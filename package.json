{
  "name": "consolens",
  "version": "1.1.5",
  "description": "Enhances JavaScript console logging with structured messages, auto-tagging, and custom icons.",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.esm.js"
    }
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcelommss/consolens.git"
  },
  "keywords": [
    "console",
    "logging",
    "javascript",
    "typescript",
    "structured-logging",
    "custom-icons"
  ],
  "author": "Marcelo Mattos (marcelomattos@hackem.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcelommss/consolens/issues"
  },
  "homepage": "https://github.com/marcelommss/consolens#readme"
}
