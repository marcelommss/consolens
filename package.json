{
  "name": "consolens",
  "version": "1.1.7",
  "description": "Enhances JavaScript console logging with structured messages, auto-tagging, contextual information, and custom icons.",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      }
    }
  },
  "dependencies": {
    "tslib": "^2.6.2",
    "process": "^0.11.10"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^22.7.6",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "rollup": "^3.27.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-typescript": "^11.1.2"
  },
  "scripts": {
    "build": "tsc && rollup -c rollup.config.mjs",
    "test": "jest",
    "prepare": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcelommss/consolens.git"
  },
  "keywords": [
    "console",
    "logging",
    "javascript",
    "typescript",
    "structured-logging",
    "custom-icons"
  ],
  "author": "Marcelo Mattos (marcelomattos@hackem.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcelommss/consolens/issues"
  },
  "homepage": "https://github.com/marcelommss/consolens#readme"
}
